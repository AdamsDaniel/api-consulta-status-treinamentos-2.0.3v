<div id="<%= titulo %>" style="width: 100%;max-width: 800px;" class="card shadow card-spreadsheets-manager">
    <div class="card-body d-flex flex-row flex-wrap">
        <div class="text-truncate fw-semibold card-spreadsheets-manager-title"><%= titulo %></div>
        <div class="d-flex flex-row gap-4 card-spreadsheets-manager-buttons">
            <button id="<%= titulo %>-edit" type="button" class="no-style-button" onclick="window.location.href='<%=url%><%=routeEdit%>'">
                <img src="/assets/icons/edit.svg" alt="editar">
            </button>
            <button id="<%= titulo %>-delete" type="button" class="no-style-button btn-notice-modal-spreadshet-<%=titulo%>">
                <img src="/assets/icons/cancel.svg" alt="excluir">
            </button>
        </div>
    </div>
</div>

